<%-include('../layout/wrapper');%>
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
	<!--begin::Toolbar-->
	<div class="toolbar" id="kt_toolbar">
		<!--begin::Container-->
		<div id="kt_toolbar_container" class="container-fluid d-flex flex-stack">
			<%-include('../layout/include/breadcrumb', {
				site
			})%>
		</div>
		<!--end::Container-->
	</div>
	<!--end::Toolbar-->
	<!--begin::Post-->
	<div class="post d-flex flex-column-fluid" id="kt_post">
		<!--begin::Container-->
        <div id="kt_content_container" class="container-xxl">
            <div class="row">
                <div class="col-md-4">
                    <!--begin::Card-->
                    <div class="card mb-5 mb-xl-10">
                        
                    </div>
                    <!--end::Card-->
                </div>
            </div>
            <!--begin::Card-->
            <div class="card">
                <!--begin::Card header-->
                <div class="card-header border-0 pt-6">
                    <!--begin::Card title-->
                    <div class="card-title">
                        <!--begin::Search-->
                        <div class="d-flex align-items-center position-relative my-1">
                            <!--begin::Svg Icon | path: icons/duotune/general/gen021.svg-->
                            <span class="svg-icon svg-icon-1 position-absolute ms-6">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                    <rect opacity="0.5" x="17.0365" y="15.1223" width="8.15546" height="2" rx="1" transform="rotate(45 17.0365 15.1223)" fill="black" />
                                    <path d="M11 19C6.55556 19 3 15.4444 3 11C3 6.55556 6.55556 3 11 3C15.4444 3 19 6.55556 19 11C19 15.4444 15.4444 19 11 19ZM11 5C7.53333 5 5 7.53333 5 11C5 14.4667 7.53333 17 11 17C14.4667 17 17 14.4667 17 11C17 7.53333 14.4667 5 11 5Z" fill="black" />
                                </svg>
                            </span>
                            <!--end::Svg Icon-->
                            <input id="kt_search" type="text" data-kt-table-filter="search" class="form-control form-control-solid w-250px ps-14" placeholder="Search <%=site.title%>..." />
                        </div>
                        <!--end::Search-->
                    </div>
                    <!--begin::Card title-->
                    <!--begin::Card toolbar-->
                    <div class="card-toolbar">
                        <!--begin::Group actions-->
                        <div id="frm-delete-multi" class="d-flex justify-content-end align-items-center me-3 d-none" data-kt-table-toolbar="selected">
                            <div class="fw-bolder me-5">
                            <span class="me-2" id="selected_count" data-kt-table-select="selected_count"></span>Selected</div>
                            <button id="btn-delete-multi" type="button" class="btn btn-danger" data-kt-table-select="delete_selected">Delete Selected</button>
                        </div>
                        <!--end::Group actions-->
                        <!--begin::Toolbar-->
                        <div class="d-flex justify-content-end" data-kt-table-toolbar="base">
                            <%-include('../data/filter')%>
                        </div>
                        <!--end::Toolbar-->
                    </div>
                    <!--end::Card toolbar-->
                </div>
                <!--end::Card header-->
                <!--begin::Card body-->
                <div class="card-body pt-0">
                    <!--begin::Table-->
                    <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_datatable">
                        <!--begin::Table head-->
                        <thead>
                            <!--begin::Table row-->
                            <tr class="text-start text-muted fw-bolder fs-7 text-uppercase gs-0">
                                <% site.datatable.listData.forEach(item => { %>
                                    <% if(item.keyIndex === "id") { %>
                                        <th class="w-25px">
                                            <div class="form-check form-check-sm form-check-custom form-check-solid">
                                                <input class="form-check-input" type="checkbox" value="1" data-kt-check="true" data-kt-check-target=".widget-13-check">
                                            </div>
                                        </th>
                                    <% } else { %>
                                        <% if(item.isAdmin) { %>
                                            <% if(session.user.ugroup === "admin") { %>
                                                <th class="min-w-125px"><%=item.title%></th>
                                            <% } %>
                                        <% } else { %>
                                            <th class="min-w-125px"><%=item.title%></th>
                                        <% } %>
                                    <% } %>
                                <% }) %>
                                <% if(site.datatable.isEdit || site.datatable.isDelete) { %>
                                    <th class="min-w-125px">Actions</th>
                                <% } %>
                            </tr>
                            <!--end::Table row-->
                        </thead>
                        <!--end::Table head-->
                    </table>
                    <!--end::Table-->
                </div>
                <!--end::Card body-->
            </div>
            <!--end::Card-->
        </div>
        <!--end::Container-->
	</div>
	<!--end::Post-->
</div>
<!--end::Content-->
<%-include('../layout/footer');%>